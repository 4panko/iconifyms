{
	"compilerOptions": {
		"rootDir": "./",
		"outDir": "./lib",
		"target": "ES2019",
		"module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"importsNotUsedAsValues": "error"
	},
  "include": ["./src/**/*.ts"]
}
