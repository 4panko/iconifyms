{
	"name": "@iconify/core",
	"description": "Reusable files used by multiple Iconify packages",
	"author": "Vjacheslav Trushkin <cyberalien@gmail.com> (https://iconify.design)",
	"version": "1.2.0-beta.2",
	"license": "(Apache-2.0 OR GPL-2.0)",
	"bugs": "https://github.com/iconify/iconify/issues",
	"homepage": "https://iconify.design/",
	"repository": {
		"type": "git",
		"url": "https://github.com/iconify/iconify.git",
		"directory": "packages/core"
	},
	"scripts": {
		"clean": "rm -rf lib compiled-tests",
		"lint": "eslint {src,tests}/**/*.ts",
		"prebuild": "npm run lint",
		"build": "tsc -b",
		"build:src": "tsc --project src/tsconfig.json",
		"prewatch": "npm run lint",
		"watch": "tsc -b -w",
		"test": "mocha tests-compiled/*/*-test.js",
		"pretest": "npm run build"
	},
	"devDependencies": {
		"@types/chai": "^4.2.16",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.14.41",
		"@types/request": "^2.48.5",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"chai": "^4.3.4",
		"eslint": "^7.24.0",
		"mocha": "^8.3.2",
		"typescript": "4.3"
	},
	"dependencies": {
		"@cyberalien/redundancy": "^1.1.0",
		"@iconify/types": "^1.0.6",
		"@iconify/utils": "^1.0.6",
		"cross-fetch": "^3.1.4"
	}
}
