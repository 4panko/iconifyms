{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"declaration": true,
		"declarationMap": false,
		"sourceMap": false,
		"composite": true,
		"strict": false,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"importsNotUsedAsValues": "error",
		"skipLibCheck": true
	}
}
